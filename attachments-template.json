{
  "template" : "attachments",
  "settings" : {
    "index.refresh_interval" : "5s"
  },
  "mappings" : {
    "*" : {
      "properties" : {
        "attachment":{"type":"binary","store":"true"},
        "filename":{"type":"keyword","store":"true"},
        "email_id":{"type":"keyword","store":"true"},
        "mime":{"type":"keyword","store":"true"},
        "size":{"type":"integer","store":"true"}
      }
    }
  }
}
